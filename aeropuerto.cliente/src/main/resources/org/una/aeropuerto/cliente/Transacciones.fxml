<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>

<AnchorPane id="AnchorPane" prefHeight="650.0" prefWidth="1000.0" stylesheets="@estilo.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.una.aeropuerto.cliente.controller.TransaccionesController">
   <children>
       <ImageView fitHeight="650.0" fitWidth="1000.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../recursos/FONDO1.png" />
         </image>
      </ImageView>
      <TableView fx:id="tvTransacciones" layoutX="63.0" layoutY="261.0" prefHeight="295.0" prefWidth="874.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnVolver" layoutX="63.0" layoutY="583.0" mnemonicParsing="false" onAction="#actVolver" text="Volver" />
      <Label layoutX="63.0" layoutY="33.0" text="Transacciones Realizadas" />
      <ComboBox fx:id="cbxRol" layoutX="63.0" layoutY="196.0" onAction="#actSelRol" prefHeight="37.0" prefWidth="256.0" promptText="Buscar por rol" />
      <Button fx:id="btnBuscarRol" layoutX="337.0" layoutY="197.0" mnemonicParsing="false" onAction="#actBuscarRol" text="Buscar" />
      <TextField fx:id="txtId" layoutX="63.0" layoutY="80.0" prefHeight="37.0" prefWidth="256.0" promptText="Buscar por id" />
      <Button fx:id="btnBuscarId" layoutX="337.0" layoutY="81.0" mnemonicParsing="false" onAction="#actBuscarId" text="Buscar" />
      <TextField fx:id="txtLugar" layoutX="63.0" layoutY="136.0" prefHeight="37.0" prefWidth="256.0" promptText="Buscar por lugar donde se realizó" />
      <Button fx:id="btnBuscarLugar" layoutX="337.0" layoutY="137.0" mnemonicParsing="false" onAction="#actBuscarLugar" text="Buscar" />
      <TextField fx:id="txtNombreUsuario" layoutX="572.0" layoutY="80.0" prefHeight="37.0" prefWidth="256.0" promptText="Buscar por nombre de usuario" />
      <Button fx:id="btnBuscarNombreUsuario" layoutX="846.0" layoutY="81.0" mnemonicParsing="false" onAction="#actBuscarNombreUsuario" text="Buscar" />
      <TextField fx:id="txtDescripcion" layoutX="572.0" layoutY="136.0" prefHeight="37.0" prefWidth="256.0" promptText="Buscar por descripción" />
      <Button fx:id="btnBuscarDescripcion" layoutX="846.0" layoutY="137.0" mnemonicParsing="false" onAction="#actBuscarDescripcion" text="Buscar" />
   </children>
</AnchorPane>
