        <Button fx:id="btnAgregar" layoutX="836.0" layoutY="562.0" mnemonicParsing="false" onAction="#actAgregar" text="Agregar" />
        <TextField fx:id="txtBuscarID" layoutX="64.0" layoutY="102.0" prefHeight="37.0" prefWidth="185.0" promptText="Buscar por ID" />
        <ComboBox fx:id="cbAreaTrabajo" layoutX="641.0" layoutY="111.0" onAction="#actAreaTrabajo" prefHeight="35.0" prefWidth="182.0" promptText="Buscar por Área trabajo" />
        <Button fx:id="btnBuscarEstado" layoutX="838.0" layoutY="166.0" mnemonicParsing="false" onAction="#actBuscarEstado" prefHeight="35.0" prefWidth="97.0" text="Buscar" />
        <Button fx:id="btnBuscarAreaTrabajo" layoutX="838.0" layoutY="111.0" mnemonicParsing="false" onAction="#actBuscarAreaTrabajo" prefHeight="35.0" prefWidth="97.0" text="Buscar" />
        <Button fx:id="btnBuscarNombre" layoutX="264.0" layoutY="151.0" mnemonicParsing="false" onAction="#actBuscarNombre" prefHeight="35.0" prefWidth="97.0" text="Buscar" />
        <TextField fx:id="txtNombre" layoutX="65.0" layoutY="150.0" prefHeight="37.0" prefWidth="185.0" promptText="Buscar por nombre" />

   </children>
