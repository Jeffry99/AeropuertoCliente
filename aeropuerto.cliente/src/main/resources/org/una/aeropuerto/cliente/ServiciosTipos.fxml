<AnchorPane id="AnchorPane" prefHeight="650.0" prefWidth="1000.0" stylesheets="@estilo.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.una.aeropuerto.cliente.controller.ServiciosTiposController">
   <children>
       <ImageView fitHeight="650.0" fitWidth="1000.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../recursos/FONDO1.png" />
         </image>
      </ImageView>
        <TableView fx:id="tvTipoServicios" layoutX="63.0" layoutY="255.0" prefHeight="274.0" prefWidth="874.0">
           <columns>
              <TableColumn prefWidth="75.0" text="C1" />
              <TableColumn prefWidth="75.0" text="C2" />
           </columns>
           <columnResizePolicy>
              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
           </columnResizePolicy>
        </TableView>
        <Button fx:id="btnVolver" layoutX="63.0" layoutY="562.0" mnemonicParsing="false" onAction="#actVolver" text="Volver" />
        <Label layoutX="63.0" layoutY="33.0" text="Tipos de Servicios" />
        <ComboBox fx:id="cbEstado" layoutX="641.0" layoutY="167.0" onAction="#actEstados" prefHeight="35.0" prefWidth="184.0" promptText="Estado" />
        <Button fx:id="btnBorrar" layoutX="840.0" layoutY="210.0" mnemonicParsing="false" onAction="#actBorrar" prefHeight="35.0" prefWidth="97.0" text="Borrar" />
