<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="org.una.aeropuerto.cliente.controller.ServiciosTiposController">
   <children>
      <AnchorPane id="AnchorPane" layoutX="10.0" layoutY="10.0" prefHeight="600.0" prefWidth="1000.0" stylesheets="@estilo.css">
         <children>
            <TableView fx:id="tvTipoServicios" layoutX="63.0" layoutY="218.0" prefHeight="274.0" prefWidth="874.0">
               <columns>
                  <TableColumn prefWidth="75.0" text="C1" />
                  <TableColumn prefWidth="75.0" text="C2" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Button fx:id="btnVolver" layoutX="63.0" layoutY="525.0" mnemonicParsing="false" onAction="#actVolver" text="Volver" />
            <Label layoutX="63.0" layoutY="33.0" text="Tipos de Servicios" />
            <ComboBox fx:id="cbEstado" layoutX="641.0" layoutY="130.0" onAction="#actEstados" prefHeight="35.0" prefWidth="184.0" promptText="Estado" />
            <Button fx:id="btnBorrar" layoutX="840.0" layoutY="173.0" mnemonicParsing="false" onAction="#actBorrar" prefHeight="35.0" prefWidth="97.0" text="Borrar" />
            <Button fx:id="btnBuscarID" layoutX="264.0" layoutY="66.0" mnemonicParsing="false" onAction="#actBuscarID" prefHeight="35.0" prefWidth="97.0" text="Buscar" />
            <Button fx:id="btnAgregar" layoutX="782.0" layoutY="525.0" mnemonicParsing="false" onAction="#actAgregar" text="Agregar Tipo Servicio" />
            <TextField fx:id="txtBuscarID" layoutX="64.0" layoutY="65.0" prefHeight="37.0" prefWidth="185.0" promptText="Buscar por ID" />
            <ComboBox fx:id="cbAreaTrabajo" layoutX="641.0" layoutY="74.0" onAction="#actAreaTrabajo" prefHeight="35.0" prefWidth="182.0" promptText="Buscar por Área trabajo" />
            <Button fx:id="btnBuscarEstado" layoutX="838.0" layoutY="129.0" mnemonicParsing="false" onAction="#actBuscarEstado" prefHeight="35.0" prefWidth="97.0" text="Buscar" />
            <Button fx:id="btnBuscarAreaTrabajo" layoutX="838.0" layoutY="74.0" mnemonicParsing="false" onAction="#actBuscarAreaTrabajo" prefHeight="35.0" prefWidth="97.0" text="Buscar" />
            <Button fx:id="btnBuscarDescripcion" layoutX="264.0" layoutY="164.0" mnemonicParsing="false" onAction="#actBuscarDescripcion" prefHeight="35.0" prefWidth="97.0" text="Buscar" />
            <TextField fx:id="txtDescripcion" layoutX="65.0" layoutY="163.0" prefHeight="37.0" prefWidth="185.0" promptText="Buscar por descripción" />
            <Button fx:id="btnBuscarNombre" layoutX="264.0" layoutY="114.0" mnemonicParsing="false" onAction="#actBuscarNombre" prefHeight="35.0" prefWidth="97.0" text="Buscar" />
            <TextField fx:id="txtNombre" layoutX="65.0" layoutY="113.0" prefHeight="37.0" prefWidth="185.0" promptText="Buscar por nombre" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
